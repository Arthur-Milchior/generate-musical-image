class TestSetOfNotes(unittest.TestCase):
    C_minor = SetOfNotes(
        [Note("C"),
         Note("E♭"),
         Note("G"),
         ])

    F_minor = SetOfNotes(
        [Note("F"),
         Note("A♭"),
         Note("C5"),
         ])

    def test_eq(self):
        self.assertNotEqual(self.C_minor, self.F_minor)
        self.assertEqual(self.C_minor, self.C_minor)

    def test_add(self):
        self.assertEqual(self.C_minor + Interval(diatonic=3, chromatic=5), self.F_minor)
